{
  "name": "torann/skosh-generator",
  "type": "library",
  "description": "Skosh is a simple static site generator for PHP using Gulp.",
  "keywords": [
    "static",
    "generator",
    "grunt",
    "jekyll",
    "static site"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "daniel@lyften.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "nesbot/carbon": "^2.0",
    "erusev/parsedown-extra": "0.8.*",
    "symfony/console": "^4.3.4",
    "symfony/filesystem": "^4.3.4",
    "symfony/finder": "^4.3.4",
    "symfony/yaml": "^4.3.4",
    "twig/twig": "1.*",
    "netcarver/textile": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "Skosh\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3-dev"
    }
  }
}
